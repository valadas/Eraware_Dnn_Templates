{
  "name": "$ext_scopeprefixkebab$",
  "version": "0.1.0",
  "private": true,
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/$ext_scopeprefixkebab$/$ext_scopeprefixkebab$.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "eslint --fix --ext .ts --ext .tsx --ext .js --ignore-pattern **/*.d.ts ./src && stencil build --docs",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "tsdoc": "tsc -d --emitDeclarationOnly ./src/services/services.ts && api-extractor run --local && api-documenter markdown -i temp -o tempmd"
  },
  "devDependencies": {
    "@eraware/dnn-elements": "^0.9.1",
    "@microsoft/api-documenter": "^7.13.49",
    "@microsoft/api-extractor": "^7.13.1",
    "@stencil/core": "^2.3.0",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.1.1",
    "@stencil/store": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-tsdoc": "^0.2.11",
    "typescript": "^4.4.3"
  },
  "license": "MIT"
}
